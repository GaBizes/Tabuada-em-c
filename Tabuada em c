// tabuada.c
// Autor: Gabriela dos Santos Ferreira
// Descrição: Programa que gera a tabuada de um número fornecido pelo usuário

#include <stdio.h>

int main() {
    int numero;

    printf("Digite um número para ver sua tabuada: ");
    scanf("%d", &numero);

    printf("\nTabuada de %d:\n", numero);
    for(int i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", numero, i, numero * i);
    }

    return 0;
}
